{
  "name": "port-heaven",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev-wb": "webpack --config webpack.config.js --watch",
    "dev-scss": "sass src/styles:src/styles --watch",
    "dev-tw": "tailwindcss -i src/styles/styles.css -o docs/styles.css --watch",
    "dev-build": "npm-run-all --parallel dev-wb dev-scss dev-tw",
    "prod-wb": "webpack --config webpack.config.js",
    "prod-scss": "sass src/styles:src/styles",
    "prod-tw": "tailwindcss -i src/styles/styles.css -o docs/styles.css --minify",
    "prod-build": "npm-run-all --serial prod-wb prod-scss prod-tw"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "copy-webpack-plugin": "^12.0.2",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.4.47",
    "sass": "^1.80.4",
    "tailwindcss": "^3.4.14",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0"
  },
  "directories": {
    "doc": "docs"
  },
  "description": ""
}
